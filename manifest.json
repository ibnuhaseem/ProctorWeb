{
  "manifest_version": 2,
  "name": "ProctorLite Timer",
  "version": "1.0",
  "description": "Timer with fullscreen enforcement.",
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
